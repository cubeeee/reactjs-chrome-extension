{
    "manifest_version": 3,
    "name": "Netroxy",
    "description": "NetProxy for chrome extension",
    "version": "1.0.0",
    "action": {
      "default_popup": "popup.html",
      "default_title": "NetProxy",
      "default_icon": "icon.png"
    },
    "permissions": [
      "bookmarks",
      "storage",
      "identity",
      "proxy",
      "storage",
      "webRequest",
      "webRequestBlocking"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "options_page": "options.html",
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ]
  }
  
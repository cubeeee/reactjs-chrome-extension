{
  "manifest_version": 3,
  "name": "Netroxy",
  "description": "NetProxy for chrome extension",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "NetProxy",
    "default_icon": "icon.png"
  },
  "permissions": [
    "proxy",
    "storage",
    "alarms",
    "webRequest",
    "webRequestAuthProvider",
    "tabs"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "incognito": "spanning",
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}